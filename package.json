{
  "name": "@finnan444/material-ui-daterange-picker",
  "version": "0.6.0",
  "description": "A react date range picker implementation using @material-ui v4 and date-fns.",
  "keywords": [
    "date",
    "range",
    "picker",
    "date-range",
    "date-range-picker",
    "daterangepicker",
    "material",
    "material-ui",
    "material design",
    "react",
    "typescript"
  ],
  "repository": "https://github.com/finnan444/materialui-daterange-picker",
  "license": "MIT",
  "author": "finnan444",
  "main": "dist/index.js",
  "module": "dist/material-ui-daterange-picker.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsdx build",
    "lint-fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.22.0"
  },
  "devDependencies": {
    "@size-limit/preset-big-lib": "^8.2.4",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.5.0",
    "typescript": "4.9.5"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/material-ui-daterange-picker.cjs.production.min.js"
    },
    {
      "path": "dist/material-ui-daterange-picker.esm.js"
    }
  ]
